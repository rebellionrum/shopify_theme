          {% endif %}
          {% if section.settings.list_3_txt != blank %}
          <li>  {{section.settings.list_3_txt}}</li>
          {% endif %}
          {% if section.settings.list_4_txt != blank %}
          <li>  {{section.settings.list_4_txt}}</li>
          {% endif %}
          {% if section.settings.list_5_txt != blank %}
