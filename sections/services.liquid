  <style>
  
		/* {% if section.settings.heading_image_need %} .service .main-heading h2:after {  content:"";background-image:url({{ section.settings.heading_image | img_url: 'medium' }});display:inline-block;background-repeat:no-repeat;width:128px;height:14px;bottom:-10px;} {% endif %} */
  
	.service .button{background: {{section.settings.serviceveda_1_pribtnbg}};color:{{section.settings.serviceveda_1_pribtncolor}};}
  
	.service .button span {background:{{section.settings.serviceveda_1_pribtnarrowbg}}; color:{{section.settings.serviceveda_1_pribtnarrowcolor}};}
  
	.service .section-one li{color:{{section.settings.serviceveda_1_listcolor}};}
  
	.service .button:hover{background:{{section.settings.servicevedabtnhoverbg}};color:{{settings.servicevedabtnhovercolor}} !important;}
  
	.service .main-heading h2{color:{{section.settings.abtsix_headingcolor}};}
  
	/* .service .main-heading p{color:{{section.settings.abtsix_subheadingcolor}};} */
  
	.service .tab-detail p{color:{{section.settings.tabdetailcolor}};}
  
	.service .tab-detail  h4{color:{{section.settings.tabdetail_title_color}};}
  
	.service .tab-icon span{color:{{section.settings.tabdetail_icon_color}};}
  
	.service  .main-title h2{color:{{section.settings.section_1_maintitlecolor}};}
  
	.service  .main-title .title-sep::after { background:{{section.settings.section_1_titlebrdcolor}};}
  
	.service  .main-title .title-sep { background:{{section.settings.section_1_titlebrdcolor}};}
  
	.service .section-four p{color:{{section.settings.section_1_desccolor}};}   
  
  
	.service .team-wrapper li a{color:{{section.settings.team_socialiconcolor}};}
  
	.service .team-wrapper p{color:{{section.settings.team_socialiconcolor}};text-decoration:underline;}
  
  
	.service .team-detail h5{color:{{section.settings.team_headingcolor}};}
  
	.service .team-wrapper li a:hover{color:{{section.settings.team_socialhoverborder}};}
  
  
	.service .team-wrapper:hover h5{color:{{section.settings.team_socialhoverborder}};}
  
	.service .section-two h2{color:{{section.settings.section_2_titleccolor}};}
  
	.service .section-two li, .service .section-two li::after{color:{{section.settings.section_2_listcolor}};}
  
	.service .section-two p{color:{{section.settings.section_2_desccolor}};}
  
	.service .section-six p{color:{{section.settings.servicevedades}};}
	
	.service .btn { color:{{ section.settings.button_1_txt }};background-color:{{ section.settings.button_1_bg}}; }
	
	.service .btn:hover { color:{{ section.settings.hover_button_1_txt}};background-color:{{ section.settings.hover_button_1_bg}}; }
	
	.service .btn:before { background-color:{{ section.settings.hover_button_1_bg}}; }
    
  </style>
  
  

  
