	{% if section.settings.show_5_section!= blank %}
	
	<div class="grid-uniform section-six " style="{% if section.settings.section_6_bgimage %} background-image:url({{section.settings.section_6_bgimage | img_url: '2048x2048' }}) {% endif %}";>
	  <div class="container">
		<div id="brand_carousel" class="owl-carousel owl-theme ">
		{% for block in section.blocks %}
		{% if block.type == 'Image' %}
		{% if block.settings.client_img != blank %}
		<div class="item">
		  <a href="{{ block.settings.client_link }}"> <img src="{{ block.settings.client_img | img_url: "250x250"  }}" alt="{{ block.settings.client_heading }}" /></a>
		</div>
		{% else %}          
		  <div class="item">
			<img src="//via.placeholder.com/200x200">
		  </div>
		{% endif %}        
		{% endif %}
		{% endfor %}
		</div>
	  </div>
	</div>   
	{% endif %}
