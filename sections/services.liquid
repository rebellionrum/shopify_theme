	<div class="invisible-large"></div>
	  <div class="container">
		<div class="grid__item">
		 
		  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item">
		   {% if section.settings.section_2_image != blank %}  <img src="{{ section.settings.section_2_image | img_url: '1024x1024' }}" alt="" title="" />
			
			{% else %}
			<img src="//via.placeholder.com/650x725">
			
			{% endif %}
		  </div>
		 
		  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item">
        {% if section.settings.section_2_rightsubtitle != blank or section.settings.section_2_righttitle  %}
          <div class="main-heading">
            <div class="short-desc"><p class="small-title">  {{section.settings.section_2_rightsubtitle }}</p></div>
            {% if section.settings.section_2_righttitle != blank %}
            <h2 style="color:{{ section.settings.section_2_righttitle_clr }};">  {{section.settings.section_2_righttitle }}</h2>
            {% endif %}
