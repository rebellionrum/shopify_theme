<div data-section-id="{{ section.id }}"  data-section-type="service section" class="service">
	{% if section.settings.showvedaone_2_section %}
	<div class="grid-uniform section-one" style="background-color:{{ section.settings.one_bgcolor }};">
  
	  <div class="container">
		<div class="grid__item">
		  
		  <div class="grid__item wide--one-half post-large--one-whole large--one-whole medium--grid__item small--grid__item">
			<div class="section-two-leftimg">
			  {% if  section.settings.abtone_image_2 != blank %}
			  <img src="{{  section.settings.abtone_image_2 | img_url: "master" }}" alt="{{ section.settings.abtone_image_2_heading }}" />
			  {% else %}
						<img src="//via.placeholder.com/858x572">
  
			   {% endif %} 
			</div>
		  </div>
		 
		  <div class="grid__item wide--one-half post-large--one-whole large--one-whole medium--grid__item small--grid__item">
			{% if section.settings.abtone_2_title != blank %}   <h2 style="color:{{section.settings.abtone_2_maintilte }}">{{ section.settings.abtone_2_title }}</h2>{% endif %} 
			{% if section.settings.abtone_2_des != blank %}
			<p style="color:{{section.settings.abtone_2_desc }}">{{section.settings.abtone_2_des }}</p>
			{% endif %}
			<div class="grid__item">
			  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item">
				<ul>
				  {% if section.settings.abtone_2_listone != blank %}  <li><span class="fa fa-caret-right"></span>{{section.settings.abtone_2_listone }} </li> {% endif %}
				  {% if section.settings.abtone_2_listtwo != blank %} <li><span class="fa fa-caret-right"></span>{{section.settings.abtone_2_listtwo }} </li>{% endif %}
				</ul>
			  </div>
			  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--grid__item small--grid__item">
				<ul>
				  {% if section.settings.abtone_2_listthree != blank %} <li><span class="fa fa-caret-right"></span>{{section.settings.abtone_2_listthree }}</li>{% endif %}
				  {% if section.settings.abtone_2_listfour != blank %}  <li><span class="fa fa-caret-right"></span>{{section.settings.abtone_2_listfour }}</li>{% endif %}
				</ul>
			  </div>
			</div>
			{% if section.settings.btn_2_text != blank %}
			<a class="btn" href="{{section.settings.link_2 }}">{{section.settings.btn_2_text }}</a>
			{% endif %} 
		  </div>
		</div>
	  </div>
		<div class="invisible-large"></div>
  
	</div> 
	{% endif %} 
  
	{% if section.settings.show_2_section != blank %}
	<div class="grid-uniform section-three" style="background:{{section.settings.service_3_bg}};float:left;width:100%;">
	  <div class="container">
		{% if section.settings.section_3_subtitle != blank or section.settings.section_3_maintitle  %}
		<div class="main-heading">
		  <div class="short-desc"><p class="small-title">  {{section.settings.section_3_subtitle }}</p></div>
		  {% if section.settings.section_3_maintitle != blank %}
		  <h2 style="color:{{ section.settings.section_3_maintitle_clr }};">  {{section.settings.section_3_maintitle }}</h2>
		  {% endif %}
		</div>
		<div class="dt-sc-hr-invisible-small"></div>
		{% endif %}
		<div class="grid__item">
		  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--one-half">
			<div class="tab-section">
			  {% if section.settings.section_3_iconcls != blank %}
			  <div class="tab-icon">
				<span class="{{section.settings.section_3_iconcls }}"> </span>
			  </div>
			  {% endif %}
			  <div class="tab-detail">
				{% if section.settings.section_3_tabtitle != blank %}
				<h4>{{section.settings.section_3_tabtitle }}</h4>
				{% endif %}
				{% if section.settings.section_3_tabdetail != blank %}
				{{section.settings.section_3_tabdetail }}
				{% endif %}
			  </div>
			</div>
		  </div>
		  <div class="grid__item wide--one-half post-large--one-half large--one-half medium--one-half">  
			<div class="tab-section">
			  {% if section.settings.section_4_iconcls != blank %}
			  <div class="tab-icon">
				<span class="{{section.settings.section_4_iconcls }}"> </span>
			  </div>
			  {% endif %}
			  <div class="tab-detail">
				{% if section.settings.section_3_tabtitleone != blank %}
				<h4>{{section.settings.section_3_tabtitleone }}</h4>
				{% endif %}
				{% if section.settings.section_3_tabdetailone != blank %}
				{{section.settings.section_3_tabdetailone }}
				{% endif %}
			  </div>
			</div>
		  </div>
		</div>      
	  </div>
	</div>
	<div class="invisible-large"></div>
	{% endif %}
  
  
	{% if section.settings.show_3_section != blank %}
	<div class="grid-uniform section-four" >
	  <div class="container">
		{% if section.settings.section_4_maintitle != blank %}
		<div class="main-title">
		  <h2>  {{section.settings.section_4_maintitle }}</h2>
		 
		</div>
		{% endif %}
		<div class="grid__item ">
		  <div class="grid__item wide--two-thirds post-large--two-thirds large--two-thirds">
			{% if section.settings.section_3_desc != blank %}
			{{section.settings.section_3_desc }}
			<div class="invisible-small"></div>
			{% endif %}
			<div class="grid__item ">
			  
			  <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-third small--grid__item">
				{% if section.settings.section_3_imageone != blank %}
				<img src="{{ section.settings.section_3_imageone | img_url: '1024x1024' }}" alt="" title="" /> 
				{% else %}
			<img src="//via.placeholder.com/500x560">
				{% endif %}
			  </div>
			  
			  
			  <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-third small--grid__item">
				{% if section.settings.section_3_imagetwo != blank %}
				<img src="{{ section.settings.section_3_imagetwo | img_url: '1024x1024' }}" alt="" title="" /> 
			  {% else %}
			<img src="//via.placeholder.com/500x560">
			  {% endif %}
			  </div>
			  
			  
			  <div class="grid__item wide--one-third post-large--one-third large--one-third medium--one-third small--grid__item">
				{% if section.settings.section_3_imagethree != blank %}
				<img src="{{ section.settings.section_3_imagethree | img_url: '1024x1024' }}" alt="" title="" /> 
				{% else %}
			<img src="//via.placeholder.com/500x560">
			  {% endif %}
			  </div>
			  
			</div>
		  </div>   
		  
		  <div class="grid__item wide--one-third post-large--one-third large--one-third">
			{% if section.settings.section_3_rightimage != blank %}
			<img src="{{ section.settings.section_3_rightimage | img_url: '1024x1024' }}" alt="" title="" />
			{% else %}
			<img src="//via.placeholder.com/600x786">
		 {% endif %}
		  </div>
		  
		</div>
	  </div>
	</div>
	<div class="invisible-large"></div>
	{% endif %}
  
  
	{% if section.settings.show_4_section != blank %}
